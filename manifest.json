{
  "manifest_version": 2,
  "name": "Hello World!",
  "version": "1.0",
  "description": "My first Chrome extension.",
  "content_scripts": [
  {
    "matches": ["https://github.com/chaluemwut/*"],
    "js": ["jquery.js", "kku.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://github.com/chaluemwut/*"
  ],  
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon":"icon.png"
  }
}